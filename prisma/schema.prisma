// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Dynamic models will be generated here by the sync service
// Following the pattern from PSD documentation

// Temporary model for deployment - will be replaced by dynamic generation
model BubbleSync {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
  @@map("bubble_sync")
}
