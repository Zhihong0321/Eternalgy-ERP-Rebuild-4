{
  "name": "eternalgy-erp-rebuild-4",
  "version": "1.0.0",
  "description": "Bubble.io to PostgreSQL sync system - Railway deployment",
  "main": "src/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "build": "cd frontend && npm ci && npm run build && cd .. && prisma generate",
    "postbuild": "echo 'Frontend built successfully' && ls -la frontend/dist/assets/",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "deploy": "npm run build && git add frontend/dist/ && echo 'Frontend built and staged for commit'"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "prisma": "^5.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "keywords": [
    "bubble.io",
    "postgresql",
    "sync",
    "railway",
    "prisma",
    "erp"
  ],
  "author": "Eternalgy Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhihong0321/Eternalgy-ERP-Rebuild-4.git"
  }
}
